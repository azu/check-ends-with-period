    "watch": "tsc -p . --watch",
    "prepublishOnly": "npm run build",
    "test": "mocha"
  },
  "dependencies": {
    "emoji-regex": "^10.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "ts-node-test-register": "^10.0.0",
    "tsconfig-to-dual-package": "^1.0.7",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@1.22.19"
}
